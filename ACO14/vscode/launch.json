{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "14.0",
    "configurations": [
        {
            "name": "Launch Debug",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Debug/ACO14",
            "args": ["${workspaceFolder}/data/NoPass.car"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch Release",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Release/ACO14",
            "args": ["${workspaceFolder}/data/NoPass.car"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}